{
  "name": "-",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "run-p dev:*",
    "dev:proxy": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "dev:next": "rimraf .cache && npm run prebuild && next dev",
    "prebuild": "ts-node prebuild.ts",
    "analyze": "ANALYZE=true npm run build",
    "build": "npx tsc -p ./tsconfig.prod.json && next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006",
    "generate-component": "node .generate-component $1",
    "specify:clean": "rm -rf ./public/fonts && rm -rf ./styles/specify && rm -rf ./assets/specify",
    "specify:lint": "stylelint --fix ./styles/specify",
    "specify": "specify sync && npm run specify:clean && specify pull && npm run specify:lint"
  },
  "dependencies": {
    "@alexisoney/storyblok-to-nextjs": "^0.0.8",
    "@next/bundle-analyzer": "^12.2.5",
    "@svgr/webpack": "^6.2.0",
    "@tailwindcss/typography": "^0.5.7",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/to-ico": "^1.1.1",
    "@types/url-join": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.10.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.7",
    "next": "^12.1.0",
    "postcss": "^8.4.14",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "^3.0.2",
    "storyblok-rich-text-react-renderer": "^2.5.2",
    "tailwindcss": "^3.1.6",
    "to-ico": "^1.1.5",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.13.0",
    "typescript": "^4.6.2",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@specifyapp/cli": "^0.0.56",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@types/uuid": "^8.3.4",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.7.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "local-ssl-proxy": "^1.3.0",
    "lodash": "^4.17.21",
    "lorem-ipsum": "^2.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "storybook-addon-next-router": "^3.1.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "uuid": "^8.3.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix --max-warnings 0",
      "prettier --write"
    ],
    "*.{css,scss}": "stylelint --fix"
  }
}
